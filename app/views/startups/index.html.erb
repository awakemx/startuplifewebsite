<p id="notice"><%= notice %></p>

<h1>Listing Startups</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Pagina</th>
      <th>Slug</th>
      <th>Startup weekend</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @startups.each do |startup| %>
      <tr>
        <td><%= startup.user %></td>
        <td><%= startup.nombre %></td>
        <td><%= startup.descripcion %></td>
        <td><%= startup.pagina %></td>
        <td><%= startup.slug %></td>
        <td><%= startup.startup_weekend %></td>
        <td><%= link_to 'Show', startup %></td>
        <td><%= link_to 'Edit', edit_startup_path(startup) %></td>
        <td><%= link_to 'Destroy', startup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Startup', new_startup_weekend_startup_path %>
